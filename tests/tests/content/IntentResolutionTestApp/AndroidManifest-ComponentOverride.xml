<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="android.content.cts.CtsIntentResolutionComponentOverrideTestApp" >

    <queries>
        <package android:name="android.content.cts" />
    </queries>

    <application android:intentMatchingFlags="enforceIntentFilter">
        <activity android:name="android.content.pm.cts.TestPmActivity"
             android:exported="true" android:intentMatchingFlags="none">
            <intent-filter>
                <action android:name="android.intent.action.RESOLUTION_TEST"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.RESOLUTION_TEST_2"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity-alias android:name="android.content.pm.cts.TestPmActivityAllowsNullAction"
            android:targetActivity="android.content.pm.cts.TestPmActivity"
            android:exported="true" android:intentMatchingFlags="allowNullAction">
            <intent-filter>
                <action android:name="android.intent.action.RESOLUTION_TEST"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.RESOLUTION_TEST_2"/>
            </intent-filter>
        </activity-alias>

        <service android:name="android.content.pm.cts.TestPmService" android:exported="true"
            android:intentMatchingFlags="none">
            <intent-filter>
                <action android:name="android.intent.action.RESOLUTION_TEST"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.RESOLUTION_TEST_2"/>
            </intent-filter>
        </service>
        <service android:name="android.content.pm.cts.TestAnotherPmService" android:exported="true"
            android:intentMatchingFlags="allowNullAction">
            <intent-filter>
                <action android:name="android.intent.action.RESOLUTION_TEST"/>
            </intent-filter>
            <intent-filter >
                <action android:name="android.intent.action.RESOLUTION_TEST_2"/>
            </intent-filter>
        </service>

        <receiver android:name="android.content.pm.cts.PmTestReceiver"
             android:exported="true" android:intentMatchingFlags="none">
            <intent-filter>
                <action android:name="android.intent.action.RESOLUTION_TEST"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.RESOLUTION_TEST_2"/>
            </intent-filter>
        </receiver>
        <receiver android:name="android.content.pm.cts.TestAnotherPmReceiver"
            android:exported="true" android:intentMatchingFlags="allowNullAction">
            <intent-filter>
                <action android:name="android.intent.action.RESOLUTION_TEST"/>
            </intent-filter>
            <intent-filter >
                <action android:name="android.intent.action.RESOLUTION_TEST_2"/>
            </intent-filter>
        </receiver>
    </application>
</manifest>
