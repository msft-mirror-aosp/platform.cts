<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.packageinstaller.multiusercontentprovider.other">

    <application
        android:label="Multiuser Test Content Provider App">
        <!--
        The authority below contains a userId (0@...). Another Content Provider
        with the same authority but without the userId part will be installed in
        user 0. This is to check if, when a caller accesses a content provider
        with a userId, it resolved to the content provider in the correct user,
        and that, the caller has the required cross user interaction permissions.
        -->
        <provider
            android:name="android.packageinstaller.contentprovider.MyTestProviderOther"
            android:authorities="0@android.packageinstaller.multiusercontentprovider"
            android:exported="true">
        </provider>
    </application>

</manifest>