<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
  <base-config>
      <certificateTransparency enabled="true" />
  </base-config>
  <domain-config>
      <domain includeSubdomains="false">foo.bar</domain>
  </domain-config>
  <domain-config>
      <domain includeSubdomains="false">google.com</domain>
      <certificateTransparency enabled="false" />
  </domain-config>
  <domain-config>
      <domain includeSubdomains="true">example.com</domain>
  </domain-config>
  <domain-config>
      <domain includeSubdomains="true">wikipedia.org</domain>
      <certificateTransparency enabled="false" />
  </domain-config>
  <domain-config>
      <domain includeSubdomains="true">custom-trust-anchor.com</domain>
      <trust-anchors>
        <certificates src="system" />
        <certificates src="user" />
      </trust-anchors>
  </domain-config>
  <domain-config>
      <domain includeSubdomains="true">ct-explicit-enabled.custom-trust-anchor.com</domain>
      <certificateTransparency enabled="true" />
      <trust-anchors>
        <certificates src="user" />
      </trust-anchors>
  </domain-config>
  <domain-config>
      <domain includeSubdomains="true">raw-resource-trust-anchor.com</domain>
      <trust-anchors>
        <certificates src="@raw/custom_ca" />
      </trust-anchors>
  </domain-config>
</network-security-config>
