package {
    default_team: "trendy_team_java_core_libraries",
    default_applicable_licenses: ["Android-Apache-2.0"],
}

android_test {
    name: "CtsLibcoreOjTestCases",
    defaults: ["cts_support_defaults"],
    platform_apis: true,
    static_libs: [
        "core-ojtests-public",

        // Jar containing expectations files.
        "libcore-expectations-knownfailures-jar",
    ],
    libs: ["testng"],
    dxflags: [
        "--core-library",
        "--android-platform-build",
    ],
    dex_preopt: {
        enabled: false,
    },
    optimize: {
        enabled: false,
    },
    // Include both the 32 and 64 bit versions of libjavacoretests,
    // where applicable.
    compile_multilib: "both",
    min_sdk_version: "31",
    target_sdk_version: "31",
    // Tag this module as a cts test artifact
    test_suites: [
        "cts",
        "general-tests",
        "mcts-art",
        "mts-art",
        "mcts-art",
    ],
    data: [
        ":CtsLibcoreTestRunner",
    ],
    per_testcase_directory: true,
    host_required: ["cts-dalvik-host-test-runner"],
    sdk_version: "test_current",
}

test_module_config {
    name: "CtsLibcoreOjTestCases_lang_invoke",
    base: "CtsLibcoreOjTestCases",
    test_suites: ["general-tests"],
    include_filters: [
        "org.openjdk.tests.java.lang.invoke",
        "test.java.lang.invoke",
    ],
}

