android_test {
    name: "CtsAppTestCases",
    team: "trendy_team_arc_app_compat",
    defaults: ["cts_defaults"],
    libs: [
        "android.test.runner.stubs.test",
        "org.apache.http.legacy.stubs.test",
        "android.test.base.stubs.test",
    ],
    static_libs: [
        "compatibility-device-util-axt",
        "ctstestrunner-axt",
        "ctstestserver",
        "cts-wm-util",
        "mockito-target-minus-junit4",
        "androidx.test.ext.junit",
        "androidx.test.rules",
        "platform-test-annotations",
        "platformprotosnano",
        "permission-test-util-lib",
        "CtsAppTestStubsShared",
        "android.content.pm.flags-aconfig-java",
        "android.multiuser.flags-aconfig-java",
        "flag-junit",
        "com.android.media.flags.bettertogether-aconfig-java",
        "android.app.flags-aconfig-java",
        "ravenwood-junit",
        "Harrier",
    ],
    srcs: [
        "src/**/*.java",
        "src/**/*.kt",
    ],
    // Tag this module as a cts test artifact
    test_suites: [
        "cts",
        "general-tests",
        "sts",
    ],
    instrumentation_for: "CtsAppTestStubs",
    sdk_version: "test_current",
    // 29 required for Harrier.
    min_sdk_version: "29",
    // Disable coverage since it pushes us over the dex limit and we don't
    // actually need to measure the tests themselves.
    jacoco: {
        exclude_filter: ["**"],
    },
    // Even with coverage disabled, we're close to the single dex limit, so allow use of multi-dex.
    dxflags: ["--multi-dex"],
    data: [
        ":CtsSimpleApp",
        ":CtsAppTestDelayedStart",
        ":CtsAppTestStubs",
        ":CtsAppTestStubsApp1",
        ":CtsAppTestStubsApp3",
        ":CtsAppTestStubsApp2",
        ":CtsAppTestStubsApi30",
        ":CtsAppTestStubsApi34",
        ":CtsAppTestWedgedStart",
        ":CtsBadProviderStubs",
        ":CtsCantSaveState1",
        ":CtsCantSaveState2",
        ":StorageDelegator",
        ":CtsActivityManagerApi29",
        ":WallpaperTest",
        ":CtsAppTestProviderApp",
    ],
    per_testcase_directory: true,
}

